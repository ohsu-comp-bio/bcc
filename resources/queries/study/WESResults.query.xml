<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="" tableDbType="TABLE">
                <tableTitle>WES Results</tableTitle>
                <!--if you include this on a table, it will apply standard configuration-->
                <javaCustomizer>org.labkey.bcc.query.BCCCustomizer</javaCustomizer>
                <columns>
                    <column columnName="date">
                        <columnTitle>Specimen Date</columnTitle>
                        <description>This should be the collection date for the associated specimen.</description>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
